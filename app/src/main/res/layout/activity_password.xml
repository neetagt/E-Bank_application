<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D59BDF"
    tools:context=".PasswordActivity">

    <Button
        android:id="@+id/BackButton2"
        android:layout_width="290dp"
        android:layout_height="50dp"
        android:layout_marginBottom="160dp"
        android:onClick="goBack"
        android:text="BACK"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.479"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/PasswordGenText"
        android:layout_width="286dp"
        android:layout_height="48dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="56dp"
        android:background="#FFFFFF"
        android:clickable="false"
        android:cursorVisible="false"
        android:ems="10"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:hint="PASSWORD"
        android:inputType="number"
        android:textColor="#070707"
        app:layout_constraintBottom_toTopOf="@+id/BackButton2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/eskiSifre2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:background="#5B1ACF"
        android:text="GENERATED PASSWORD"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/PasswordGenText"
        app:layout_constraintStart_toStartOf="@+id/PasswordGenText" />
</androidx.constraintlayout.widget.ConstraintLayout>